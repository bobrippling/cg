type $a = {i4,i4,i1*,i1*}

$s = [i1 x 0] internal "hello\0"
$s2 = [i1 x 0] internal "hello 2\0"

$f = $a() {
	$stack = alloca $a
	$p1 = elem $stack, i8 0
	$p2 = elem $stack, i8 1
	store $p1, i4 1
	store $p2, i4 2

	$p3 = elem $stack, i8 2
	$p4 = elem $stack, i8 3

	$s_0 = elem $s, i8 0
	store $p3, $s_0
	$s2_0 = elem $s2, i8 0
	store $p4, $s2_0

	$tmp = load $stack
	ret $tmp
}
