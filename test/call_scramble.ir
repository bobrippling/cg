# test=harness 0

$x = i4

$f = i4(i4 $a, i4 $b, i4 $c, i4 $d)
{
	$a_ = eq $a, i4 9
	br $a_, $next1, $abort

$next1:
	$b_ = eq $b, i4 2
	br $b_, $next2, $abort

$next2:
	$c_ = eq $c, i4 7
	br $b_, $next3, $abort

$next3:
	$d_ = eq $d, i4 3
	br $b_, $next4, $abort

$next4:
	ret i4 0

$abort:
	ret i4 1
}

$test = i4()
{
	$a_ = load $x
	$a = add $a_, i4 9

	$b_ = load $x
	$b = add $b_, i4 2

	$c_ = load $x
	$c = add $c_, i4 7

	$d_ = load $x
	$d = add $d_, i4 3
	
	$r = call $f($a, $b, $c, $d) # shouldn't mix up registers when assigning
	
	ret $r
}
